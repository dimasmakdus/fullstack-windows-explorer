{
  "name": "windows-explorer-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx vue-tsc --noEmit && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "type-check": "bunx vue-tsc --noEmit",
    "lint": "bunx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "bunx prettier --write src/",
    "clean": "rm -rf dist",
    "install:clean": "rm -rf node_modules bun.lockb && bun install",
    "start": "bun run dev"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.19.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "@vitejs/plugin-vue",
    "vue-tsc",
    "vite"
  ]
}